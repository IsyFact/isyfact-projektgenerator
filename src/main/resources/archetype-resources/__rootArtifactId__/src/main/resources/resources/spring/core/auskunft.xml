#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
                        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	<!-- ======================================================================
		Dies ist eine Spring Konfigurationsdatei fuer alle fachlichen Komponenten
		des Registers. 
		
		Ueblicherweise wird pro Komponente eine Bean fuer eine Verwalter-Klasse
		konfiguriert. Diese Verwalter-Klasse instanziiert dann das Objekt-Geflecht
		der Komponente. Bei Bedarf koennen weitere Beans erstellt werden.
		====================================================================== -->

	<!-- ======================================================================
		Beans der Komponente "Auskunft".
		====================================================================== -->
	<bean id="auskunft" class="${package}.core.auskunft.impl.AuskunftImpl">
		<property name="awfRegisterEintragSuchen" ref="awfRegisterEintragSuchen"/>
	</bean>

	<bean id="awfRegisterEintragSuchen" class="${package}.core.auskunft.impl.AwfRegisterEintragSuchen"/>

</beans>
